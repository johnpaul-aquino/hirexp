{
  "name": "hirexp-landingpage",
  "version": "1.0.0",
  "description": "HireXp - AI-powered English training platform landing page connecting job seekers with call center companies",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "export": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:build": "docker compose build",
    "docker:rebuild": "docker compose up -d --build",
    "docker:logs": "docker compose logs -f",
    "docker:logs:app": "docker compose logs -f app",
    "docker:db:migrate": "docker compose exec app npx prisma migrate dev",
    "docker:db:studio": "docker compose exec app npx prisma studio",
    "docker:db:seed": "docker compose exec app npx prisma db seed",
    "docker:db:reset": "docker compose exec app npx prisma migrate reset",
    "docker:clean": "docker compose down -v"
  },
  "keywords": [
    "nextjs",
    "landing-page",
    "ai-training",
    "call-center"
  ],
  "author": "",
